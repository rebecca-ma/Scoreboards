<%= render 'shared/errors', container: @mission %>

<%= form_for @mission do |f| %>
  <p>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.label 'Assigned by' %>
    <%= f.text_field :giver %>
  </p>
  <p>
    <%= f.submit submit_msg %>
  </p>
<% end %>
